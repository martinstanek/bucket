{
  "Info": {
    "Name": "GfmsMachineMonitor",
    "Description": "GFMS Machine Monitor",
    "Version": "0.0.1"
  },
  "Configuration": {
    "FetchImages": true
  },
  "Parameters": [
    {
      "Name": "gfmsborneocr_password",
      "Description": "User password for the gfmsborneocr.azurecr.io registry"
    }
  ],
  "Registries": [
    {
      "Name": "docker",
      "Server": "docker.io",
      "User": "",
      "Password": ""
    },
    {
      "Name": "gfmsborneocr",
      "Server": "gfmsborneocr.azurecr.io",
      "User": "gfmsborneocr",
      "Password": "#{gfmsborneocr_password}#"
    }
  ],
  "Images": [
    {
      "Alias": "nginx",
      "FullName": "docker.io/nginx:1.23.4"
    },
    {
      "Alias": "mongo",
      "FullName": "docker.io/mongo:7.0.12"
    }
  ],
  "Stacks": [
    "./borneo",
    "./infra"
  ]
}